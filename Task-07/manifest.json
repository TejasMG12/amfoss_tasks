{
  "manifest_version": 3,
  "name": "Weather",
  "version": "1.0",
  "description": "Weather showing Extension",
  "permissions": [
    "activeTab",
    "geolocation"
  ],
  "commands": {
    "open-extension": {
      "suggested_key": {
        "default": "Ctrl+L",
        "mac": "Command+L"
      },
      "description": "Open My Extension"
    }
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/logo.png",
      "48": "assets/logo.png",
      "128": "assets/logo.png"
    }
  },
  "icon": {
    "16": "assets/logo.png",
    "48": "assets/logo.png",
    "128": "assets/logo.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/logo.png",
      "48": "assets/logo.png",
      "128": "assets/logo.png"
    }
  }
}
